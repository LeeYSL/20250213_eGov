<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace ="egovframework.example.board.service.BoardMapper">
 
 
 <!--  게시글 등록 -->
 <insert id="insertBoard" parameterType="com.egov.vo.BoardVO">
 	INSER INTO meetingMain (meeting_id,title,writer,emp_id,meeting_date,location,attendees,content,meeting_issue,action_note,action_date,action_person)
 	VALUES (#{meeting_id},#{title},#{writer},#{emp_id},#{meeting_date},#{location},
 			#{attendees},#{content},#{meeting_issue},#{action_note},#{action_date},#{action_person})
 </insert>
 
 
 
 
 
 
 
 
 
 
 </mapper>